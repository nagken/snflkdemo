# Snowflake Connection Configuration
# Replace with your actual Snowflake credentials

SF_ACCOUNT=your_account.region
SF_USER=your_username
SF_PASSWORD=your_password
SF_ROLE=ACCOUNTADMIN
SF_WAREHOUSE=GENAI_WH
SF_DATABASE=GENAI_DB
SF_SCHEMA=PUBLIC

# Optional: Snowflake Private Key Authentication
# SF_PRIVATE_KEY_PATH=/path/to/private_key.p8
# SF_PRIVATE_KEY_PASSPHRASE=your_passphrase

# Cortex Configuration
CORTEX_EMBEDDING_MODEL=text-embedding-ada-002
CORTEX_LLM_MODEL=mistral-large

# Telemetry Settings
ENABLE_TELEMETRY=true
LOG_LEVEL=INFO